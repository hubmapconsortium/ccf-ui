    "@angular/router": "^12.1.1",
    "@angular/youtube-player": "^12.1.1",
    "@comunica/query-sparql-rdfjs": "^2.2.0",
    "@deck.gl/core": "^8.5.7",
    "@deck.gl/geo-layers": "^8.7.8",
    "@deck.gl/layers": "^8.5.7",
    "@deck.gl/mesh-layers": "^8.5.7",
    "@loaders.gl/core": "^3.0.12",
    "@loaders.gl/draco": "^3.0.12",
    "@loaders.gl/gltf": "^3.0.12",
    "@luma.gl/core": "^8.5.7",
    "@math.gl/core": "^3.5.5",
    "@math.gl/culling": "^3.5.7",
    "@ngxs-labs/data": "^6.2.0",
    "@ngxs/logger-plugin": "^3.7.1",
    "@ngxs/storage-plugin": "^3.7.1",
    "@ngxs/store": "^3.7.3",
    "@rdfjs/data-model": "^1.2.0",
    "@types/lru-cache": "<7.0.0",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "bind-decorator": "^1.0.11",
    "body-parser": "^1.19.0",
