{
  "name": "ccf-ui",
  "version": "1.0.0-alpha.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build:demo": "ng build --prod --output-hashing=none --base-href=./",
    "build:doc": "npm run compodoc",
    "build:libs": "npm-run-all build:triple-store-utils build:ccf-database",
    "build:ccf-database": "npm run build ccf-database",
    "build:triple-store-utils": "npm run build triple-store-utils && npm run fix-triple-store-utils",
    "fix-triple-store-utils": "rimraf dist/triple-store-fixed && mkdirp dist/triple-store-fixed && browserify -s tripleStoreUtils dist/triple-store-utils/bundles/triple-store-utils.umd.js -o dist/triple-store-fixed/index.js && cp dist/triple-store-utils/lib/triple-store-utils.d.ts dist/triple-store-fixed/index.d.ts",
    "build:netlify": "npm-run-all build:libs build cp-netlify-redirects compodoc",
    "cp-netlify-redirects": "cp src/_redirects dist/ccf-ui/_redirects",
    "compodoc": "npx compodoc -p src/tsconfig.app.json --output dist/ccf-ui/docs",
    "test": "ng test",
    "test:wsl": "ng test --browsers=ChromeHeadlessWSL",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sonar": "sonar-scanner"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.2",
    "@angular/cdk": "^9.2.1",
    "@angular/common": "~9.1.2",
    "@angular/compiler": "~9.1.2",
    "@angular/core": "~9.1.2",
    "@angular/forms": "~9.1.2",
    "@angular/material": "^9.2.1",
    "@angular/platform-browser": "~9.1.2",
    "@angular/platform-browser-dynamic": "~9.1.2",
    "@angular/router": "~9.1.2",
    "@ngxs-labs/data": "^2.4.1",
    "@ngxs/store": "^3.6.2",
    "@rdfjs/data-model": "^1.1.2",
    "jsonld": "^3.1.0",
    "lodash": "^4.17.15",
    "n3": "^1.3.6",
    "ng5-slider": "^1.2.4",
    "rdf-literal": "^1.1.0",
    "rdfxml-streaming-parser": "^1.3.5",
    "rxjs": "~6.5.5",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular-devkit/build-ng-packagr": "~0.901.1",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.2",
    "@angular/language-service": "~9.1.2",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/js-yaml": "^3.12.3",
    "@types/jsonld": "^1.5.1",
    "@types/lodash": "^4.14.150",
    "@types/n3": "^1.1.6",
    "@types/node": "^13.13.1",
    "@types/rdf-js": "^3.0.0",
    "@types/readable-stream": "^2.3.5",
    "@types/sax": "^1.2.1",
    "browserify": "^16.5.1",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "^5.0.1",
    "karma": "^5.0.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "mkdirp": "^1.0.4",
    "ng-packagr": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.4",
    "rimraf": "^3.0.2",
    "shallow-render": "^9.0.4",
    "sonar-scanner": "^3.1.0",
    "ts-node": "~8.3.0",
    "tslint": "^6.1.1",
    "typescript": "~3.7.5"
  }
}
