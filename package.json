    "document-register-element": "^1.7.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "file-saver": "^2.0.5",
    "fs-extra": "^10.0.0",
    "graphology": "^0.20.0",
    "graphology-shortest-path": "^1.4.1",
    "helmet": "^4.6.0",
    "hex-rgb": "^5.0.0",
    "idb-keyval": "^6.0.1",
    "jsonld": "^5.2.0",
    "lodash": "^4.17.21",
    "lru-cache": "<7.0.0",
    "n3": "^1.16.0",
    "ng5-slider": "^1.2.6",
    "ngx-google-analytics": "^11.2.1",
    "ngx-markdown": "^12.0.1",
    "node-fetch": "^2.6.7",
    "qs": "^6.10.2",
    "rdf-literal": "^1.2.0",
