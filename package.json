{
  "name": "ccf-ui",
  "version": "1.0.0-rc.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build:demo": "ng build --prod --output-hashing=none --base-href=./",
    "build:doc": "npx compodoc -p tsconfig.compodoc.json --output dist/ccf-ui/docs",
    "build:doc:ccf-ui": "npx compodoc -p src/tsconfig.app.json --output dist/ccf-ui/docs",
    "build:doc:ccf-body-ui": "npx compodoc -p projects/ccf-body-ui/tsconfig.lib.json --output dist/ccf-body-ui/docs",
    "build:doc:ccf-database": "npx compodoc -p projects/ccf-database/tsconfig.lib.json --output dist/ccf-database/docs",
    "build:doc:ccf-image-viewer": "npx compodoc -p projects/ccf-image-viewer/tsconfig.lib.json --output dist/ccf-image-viewer/docs",
    "build:doc:triple-store-utils": "npx compodoc -p projects/triple-store-utils/tsconfig.lib.json --output dist/triple-store-utils/docs",
    "build:libs": "npm-run-all build:triple-store-utils build:ccf-database build:ccf-body-ui build:ccf-image-viewer",
    "build:ccf-body-ui": "npm run build ccf-body-ui",
    "build:ccf-database": "npm run build ccf-database",
    "build:ccf-image-viewer": "npm run build ccf-image-viewer",
    "build:hubmap-jsonld": "ts-node --project projects/ccf-database/tsconfig.cli.json projects/ccf-database/src/process-hubmap-data.ts ./src/assets/dev-data/entities.json ./src/assets/dev-data/entities.jsonld",
    "build:triple-store-utils": "npm run build triple-store-utils && npm run fix-triple-store-utils",
    "fix-triple-store-utils": "rimraf dist/triple-store-fixed && mkdirp dist/triple-store-fixed && browserify -s tripleStoreUtils dist/triple-store-utils/bundles/triple-store-utils.umd.js -o dist/triple-store-fixed/index.js && cp dist/triple-store-utils/lib/triple-store-utils.d.ts dist/triple-store-fixed/index.d.ts",
    "fix-types": "npm-run-all fix-types:* fix-types-final:deck-gl",
    "fix-types:quaternion": "dts-gen -o -m quaternion -f node_modules/quaternion/quaternion.d.ts",
    "fix-types:quaternion-to-euler": "dts-gen -o -m quaternion-to-euler -f node_modules/quaternion-to-euler/index.d.ts",
    "fix-types:vitessce-image-viewer": "mkdir -p node_modules/@types/@hubmap/vitessce-image-viewer && cp projects/ccf-image-viewer/types/@hubmap/vitessce-image-viewer/index.d.ts node_modules/@types/@hubmap/vitessce-image-viewer/index.d.ts",
    "fix-types-final:deck-gl": "npm install --save-dev @danmarshall/deckgl-typings",
    "build:netlify": "npm-run-all fix-types build:libs build cp-netlify-redirects compodoc",
    "cp-netlify-redirects": "cp src/_redirects dist/ccf-ui/_redirects",
    "compodoc": "npm run build:doc",
    "test": "ng test",
    "test:wsl": "ng test --browsers=ChromeHeadlessWSL",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sonar": "sonar-scanner"
  },
  "private": true,
  "browser": {
    "fs": false
  },
  "dependencies": {
    "@angular/animations": "^9.1.11",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.11",
    "@angular/compiler": "^9.1.11",
    "@angular/core": "^9.1.11",
    "@angular/forms": "^9.1.11",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.11",
    "@angular/platform-browser-dynamic": "^9.1.11",
    "@angular/router": "^9.1.11",
    "@deck.gl/core": "^8.1.12",
    "@deck.gl/geo-layers": "^8.1.12",
    "@deck.gl/layers": "^8.1.12",
    "@deck.gl/mesh-layers": "^8.1.12",
    "@hubmap/vitessce-image-viewer": "0.2.8",
    "@loaders.gl/core": "^2.1.6",
    "@loaders.gl/draco": "^2.1.5",
    "@loaders.gl/gltf": "^2.1.6",
    "@luma.gl/core": "^8.1.3",
    "@math.gl/core": "3.2.0-alpha.3",
    "@ngxs-labs/data": "^3.0.5",
    "@ngxs/store": "^3.6.2",
    "@rdfjs/data-model": "^1.1.2",
    "bind-decorator": "^1.0.11",
    "comlink": "^4.3.0",
    "css-element-queries": "^1.2.3",
    "graphology": "^0.17.1",
    "graphology-shortest-path": "^1.3.3",
    "jsonld": "^3.1.1",
    "lodash": "^4.17.19",
    "n3": "^1.4.0",
    "ng5-slider": "^1.2.4",
    "quaternion": "^1.1.0",
    "quaternion-to-euler": "^0.5.0",
    "rdf-literal": "^1.1.1",
    "rdfxml-streaming-parser": "^1.3.6",
    "react": "^16.13.1",
    "rxjs": "~6.5.5",
    "tslib": "^1.13.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.9",
    "@angular-devkit/build-ng-packagr": "^0.901.9",
    "@angular/cli": "^9.1.9",
    "@angular/compiler-cli": "^9.1.11",
    "@angular/language-service": "^9.1.11",
    "@compodoc/compodoc": "^1.1.11",
    "@danmarshall/deckgl-typings": "^4.1.9",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/js-yaml": "^3.12.4",
    "@types/jsonld": "^1.5.1",
    "@types/lodash": "^4.14.155",
    "@types/n3": "^1.1.6",
    "@types/node": "^14.0.13",
    "@types/rdf-js": "^3.0.1",
    "@types/readable-stream": "^2.3.7",
    "@types/sax": "^1.2.1",
    "browserify": "^16.5.1",
    "codelyzer": "^5.2.2",
    "dts-gen": "^0.6.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "mkdirp": "^1.0.4",
    "ng-packagr": "^9.1.5",
    "npm-run-all": "^4.1.5",
    "protractor": "^7.0.0",
    "rimraf": "^3.0.2",
    "shallow-render": "^9.0.5",
    "sonar-scanner": "^3.1.0",
    "ts-node": "~8.3.0",
    "tslint": "^6.1.2",
    "typescript": "~3.7.5"
  }
}
