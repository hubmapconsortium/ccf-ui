{
  "name": "ccf-ui",
  "version": "1.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build:demo": "ng build --prod --output-hashing=none --base-href=./",
    "build:doc": "npx compodoc -p tsconfig.compodoc.json --output dist/ccf-ui/docs",
    "build:doc:ccf-eui": "npx compodoc -p projects/ccf-eui/tsconfig.app.json --output dist/ccf-eui/docs",
    "build:doc:ccf-rui": "npx compodoc -p projects/ccf-rui/tsconfig.app.json --output dist/ccf-rui/docs",
    "build:doc:ccf-body-ui": "npx compodoc -p projects/ccf-body-ui/tsconfig.lib.json --output dist/ccf-body-ui/docs",
    "build:doc:ccf-database": "npx compodoc -p projects/ccf-database/tsconfig.lib.json --output dist/ccf-database/docs",
    "build:doc:ccf-image-viewer": "npx compodoc -p projects/ccf-image-viewer/tsconfig.lib.json --output dist/ccf-image-viewer/docs",
    "build:doc:triple-store-utils": "npx compodoc -p projects/triple-store-utils/tsconfig.lib.json --output dist/triple-store-utils/docs",
    "build:libs": "npm-run-all build:triple-store-utils build:ccf-database build:ccf-body-ui build:ccf-image-viewer build:ccf-shared",
    "build:ccf-body-ui": "npm run build ccf-body-ui",
    "build:ccf-database": "npm run build ccf-database",
    "build:ccf-image-viewer": "npm run build ccf-image-viewer",
    "build:ccf-shared": "npm run build ccf-shared",
    "build:hubmap-jsonld": "ts-node --project projects/ccf-database/tsconfig.cli.json projects/ccf-database/src/process-hubmap-data.ts ./src/assets/dev-data/entities.json ./src/assets/dev-data/entities.jsonld",
    "build:triple-store-utils": "npm run build triple-store-utils && npm run fix-triple-store-utils",
    "generate-reference-data": "ts-node --project projects/ccf-body-ui/tsconfig.cli.json projects/ccf-body-ui/src/generate-reference-data.ts ../hubmap-ontology/source_data/reference-spatial-entities.jsonld ../hubmap-ontology/source_data/generated-reference-spatial-entities.jsonld",
    "precompute-reference-data": "ts-node --project projects/ccf-database/tsconfig.cli.json projects/ccf-database/src/precompute-reference-data.ts projects/ccf-rui/src/assets/reference-organ-data.json",
    "reference-data-workflow": "npm run generate-reference-data && cd ../hubmap-ontology/ && ./build.sh && cd ../ccf-ui/ && npm run precompute-reference-data",
    "fix-triple-store-utils": "rimraf dist/triple-store-fixed && mkdirp dist/triple-store-fixed && browserify -s tripleStoreUtils dist/triple-store-utils/bundles/triple-store-utils.umd.js -o dist/triple-store-fixed/index.js && cp dist/triple-store-utils/lib/triple-store-utils.d.ts dist/triple-store-fixed/index.d.ts",
    "fix-types": "npm-run-all fix-types:* fix-types-final:deck-gl",
    "fix-types:quaternion": "dts-gen -o -m quaternion -f node_modules/quaternion/quaternion.d.ts",
    "fix-types:quaternion-to-euler": "dts-gen -o -m quaternion-to-euler -f node_modules/quaternion-to-euler/index.d.ts",
    "fix-types:vitessce-image-viewer": "mkdir -p node_modules/@types/@hms-dbmi/viv && cp projects/ccf-image-viewer/types/@hms-dbmi/viv/index.d.ts node_modules/@types/@hms-dbmi/viv/index.d.ts",
    "fix-types-final:deck-gl": "npm install --save-dev @danmarshall/deckgl-typings@latest",
    "build:netlify": "npm-run-all fix-types build:libs build cp-netlify-redirects compodoc",
    "cp-netlify-redirects": "cp projects/ccf-eui/_redirects dist/ccf-eui/_redirects",
    "compodoc": "npm run build:doc",
    "test": "ng test",
    "test:wsl": "ng test --browsers=ChromeHeadlessWSL",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sonar": "sonar-scanner"
  },
  "private": true,
  "browser": {
    "fs": false
  },
  "dependencies": {
    "@angular/animations": "^10.0.7",
    "@angular/cdk": "^10.1.1",
    "@angular/common": "^10.0.7",
    "@angular/compiler": "^10.0.7",
    "@angular/core": "^10.0.7",
    "@angular/forms": "^10.0.7",
    "@angular/material": "^10.1.1",
    "@angular/platform-browser": "^10.0.7",
    "@angular/platform-browser-dynamic": "^10.0.7",
    "@angular/router": "^10.0.7",
    "@angular/youtube-player": "^10.1.3",
    "@deck.gl/core": "^8.2.8",
    "@deck.gl/geo-layers": "^8.2.8",
    "@deck.gl/layers": "^8.2.8",
    "@deck.gl/mesh-layers": "^8.2.8",
    "@hms-dbmi/viv": "^0.4.2",
    "@loaders.gl/core": "^2.2.8",
    "@loaders.gl/draco": "^2.2.8",
    "@loaders.gl/gltf": "^2.2.8",
    "@luma.gl/core": "^8.2.0",
    "@math.gl/core": "^3.2.2",
    "@ngxs-labs/data": "^3.0.6",
    "@ngxs/logger-plugin": "^3.6.2",
    "@ngxs/storage-plugin": "^3.7.0",
    "@ngxs/store": "^3.6.2",
    "@rdfjs/data-model": "^1.1.2",
    "bind-decorator": "^1.0.11",
    "cannon-es": "^0.15.1",
    "comlink": "^4.3.0",
    "css-element-queries": "^1.2.3",
    "file-saver": "^2.0.2",
    "graphology": "^0.17.1",
    "graphology-shortest-path": "^1.3.4",
    "jsonld": "^3.1.1",
    "lodash": "^4.17.19",
    "n3": "^1.6.1",
    "ng5-slider": "^1.2.4",
    "quaternion": "^1.1.0",
    "quaternion-to-euler": "^0.5.0",
    "rdf-literal": "^1.1.1",
    "rdfxml-streaming-parser": "^1.3.6",
    "react": "^16.13.1",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "uuid": "^8.3.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.4",
    "@angular-devkit/build-ng-packagr": "^0.1000.5",
    "@angular/cli": "^10.0.5",
    "@angular/compiler-cli": "^10.0.7",
    "@angular/language-service": "^10.0.7",
    "@compodoc/compodoc": "^1.1.11",
    "@danmarshall/deckgl-typings": "^4.4.0",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "^3.5.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/js-yaml": "^3.12.5",
    "@types/jsonld": "^1.5.1",
    "@types/lodash": "^4.14.158",
    "@types/n3": "^1.4.3",
    "@types/node": "^14.0.27",
    "@types/node-fetch": "^2.5.7",
    "@types/papaparse": "^5.2.2",
    "@types/rdf-js": "^3.0.3",
    "@types/readable-stream": "^2.3.9",
    "@types/sax": "^1.2.1",
    "@types/uuid": "^8.3.0",
    "browserify": "^16.5.1",
    "codelyzer": "^6.0.0",
    "dts-gen": "^0.6.0",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "mkdirp": "^1.0.4",
    "ng-packagr": "^10.0.0",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "papaparse": "^5.3.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "shallow-render": "^10.0.1",
    "sonar-scanner": "^3.1.0",
    "ts-node": "~8.3.0",
    "tslint": "^6.1.3",
    "typescript": "~3.9.7"
  }
}
