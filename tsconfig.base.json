{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist/out-tsc",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": true,
    "strictBindCallApply": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "importHelpers": true,
    "skipLibCheck": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types",
      "node_modules/@danmarshall"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ccf-body-ui": [
        "libs/ccf-body-ui/src/public-api.ts"
      ],
      "ccf-database": [
        "libs/ccf-database/src/public-api.ts"
      ],
      "ccf-shared": [
        "libs/ccf-shared/src/public-api.ts"
      ],
      "ccf-shared/analytics": [
        "libs/ccf-shared/analytics/src/public-api.ts"
      ],
      "ccf-shared/rxjs-ext": [
        "libs/ccf-shared/rxjs-ext/src/public-api.ts"
      ],
      "ccf-shared/rxjs-ext/operators": [
        "libs/ccf-shared/rxjs-ext/operators/src/public-api.ts"
      ],
      "ccf-shared/web-components": [
        "libs/ccf-shared/web-components/src/public-api.ts"
      ],
      "stream": [
        "./node_modules/stream-browserify"
      ],
      "triple-store-utils": [
        "libs/triple-store-utils/src/public-api.ts"
      ]
    },
    "useDefineForClassFields": false,
    "rootDir": "."
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
